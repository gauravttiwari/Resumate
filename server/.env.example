```bash
# Server Configuration
PORT=5000

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000

# File Storage Settings
STORAGE_PATH=./storage

# Debug Mode (true/false)
DEBUG=false

# Google Gemini AI API Settings
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-pro  # Default model - gemini-pro for text, gemini-pro-vision for multimodal

# ATS Optimization Settings
ATS_MIN_SCORE_THRESHOLD=70  # Minimum score for ATS compatibility
```
